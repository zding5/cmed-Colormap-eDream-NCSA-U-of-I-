Function {make_window()} {open
} {
  Fl_Window {} {open
    xywh {655 251 400 610} type Double hide resizable
  } {
    Fl_Button undobutton {
      label Undo
      callback undo_cb
      xywh {6 25 40 25} down_box DOWN_BOX
