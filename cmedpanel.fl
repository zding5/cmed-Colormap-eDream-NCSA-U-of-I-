      xywh {206 25 75 25} type {Horz Knob} selection_color 1 align 8 minimum 0.05 value 0.5
    }
    Fl_Button rgbmode {
      label HSB
      callback rgbmode_cb
      xywh {331 25 35 25} labelsize 12 align 16
