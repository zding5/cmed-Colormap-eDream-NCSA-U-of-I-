    Fl_Box {cshow.hsblbls[0]} {
      label {Hue (L)}
      xywh {40 219 80 20} box FLAT_BOX color 34 labelfont 1 labelsize 12 labelcolor 3
    }
    Fl_Box {cshow.hsblbls[1]} {
      label {Sat(M)}
      xywh {125 219 75 20} box FLAT_BOX color 34 labelfont 1 labelsize 12 labelcolor 2
    }
    Fl_Box {cshow.hsblbls[2]} {
      label {Bright(R)}
      xywh {205 219 75 20} box FLAT_BOX color 34 labelfont 1 labelsize 12 labelcolor 235
    }
    Fl_Box {} {
      label {Alpha(shift)}
      xywh {285 219 80 20} box FLAT_BOX color 34 labelfont 1 labelsize 12 labelcolor 7
    }
    Fl_Box cmedit {
      xywh {5 239 380 270} box FLAT_BOX resizable
      class {class CMedit}
    }
    Fl_Box {cshow.color} {
      xywh {72 515 50 40} box DOWN_BOX labeltype NO_LABEL
      class colorpatch
    }
    Fl_Value_Input {cshow.cindex} {
      label {  (index}
      callback report_cb
      xywh {17 530 50 25} box FLAT_BOX color 16 align 1 maximum 0 step 1 textsize 12
    }
    Fl_Output {cshow.hsba} {
      label HSBA
      xywh {127 530 120 25} box FLAT_BOX color 16 align 1 textsize 11
    }
    Fl_Output {cshow.rgba} {
      label rgb
      xywh {257 530 125 25} box FLAT_BOX color 16 align 1 textsize 11
    }
    Fl_Value_Input {cshow.cmentin} {
      label cmapsize
      callback ncment_cb
      xywh {17 570 55 25} box ENGRAVED_BOX color 25 align 5 when 4
    }
    Fl_Menu_Bar {} {open
      xywh {0 0 400 20} color 46
    } {
      Submenu menu_file {
        label {&File} open
        xywh {0 0 70 21}
      } {
        MenuItem save_color_file {
          label {Save Color File... }
          callback menu_fsave_cb
          xywh {0 0 36 21} shortcut 0x40073
        }
        MenuItem load_color_file {
          label {Load Color File...}
          callback menu_fload_cb
          xywh {0 0 36 21} shortcut 0x4006c
        }
        MenuItem {} {
          label {load histogram file...}
          callback menu_histfload_cb
          xywh {0 0 36 21} shortcut 0x5006c
        }
      }
      Submenu {} {
        label {&Edit} open
        xywh {0 0 70 21}
      } {
        MenuItem menu_undo {
          label Undo
          callback menu_undo_cb
          xywh {0 0 36 21} shortcut 0x4007a
        }
        MenuItem menu_redo {
          label Redo
          callback menu_redo_cb
          xywh {0 0 36 21} shortcut 0x40079
        }
      }
      Submenu {} {
        label {&Zoom} open
        xywh {0 0 70 21}
      } {
        MenuItem zoom_panel {
          label {Zoom Panel}
          callback zoom_panel_cb
          xywh {0 0 36 21}
        }
      }
      Submenu {} {
        label {&Modes} open
        xywh {0 0 70 21}
      } {
        MenuItem {} {
          label {Drawing Mode}
          user_data 1 user_data_type long
          callback change_mode
          xywh {0 0 36 21}
          code0 {o->shortcut(FL_SHIFT + FL_ALT + 'd');}
        }
        MenuItem {} {
          label {&Scaling Mode}
          user_data 2 user_data_type long
          callback change_mode
          xywh {0 0 36 21}
          code0 {o->shortcut(FL_SHIFT + FL_ALT + 's');}
        }
        MenuItem {} {
          label {Interpolation Mode}
          user_data 3 user_data_type long
          callback change_mode selected
          xywh {0 0 36 21}
        }
      }
    }
    Fl_Button {} {
      label Reset
      callback display_reset_cb
      xywh {55 25 70 25}
    }
  }
} 

